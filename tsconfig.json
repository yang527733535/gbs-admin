{
  "compilerOptions": {
    "moduleResolution": "node",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "react",
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "module": "es2020",
    "target": "es5",
    "declaration": false,
    "outDir": "es",
    "lib": [
      "es5",
      "dom"
    ],
    "types": [
      "node",
      "jest"
    ],
    "resolveJsonModule": true
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "src/utils/getdicts.js"
  ],
  "exclude": [
    "node_modules"
  ]
}